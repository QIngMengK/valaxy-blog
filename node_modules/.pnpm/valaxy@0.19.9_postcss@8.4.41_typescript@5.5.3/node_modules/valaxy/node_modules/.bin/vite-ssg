#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/blog/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0.0.24_vite@5.4.2_vue-router@4.4.3_vue@3.4.38/node_modules/vite-ssg/bin/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0.0.24_vite@5.4.2_vue-router@4.4.3_vue@3.4.38/node_modules/vite-ssg/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0.0.24_vite@5.4.2_vue-router@4.4.3_vue@3.4.38/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/blog/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0.0.24_vite@5.4.2_vue-router@4.4.3_vue@3.4.38/node_modules/vite-ssg/bin/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0.0.24_vite@5.4.2_vue-router@4.4.3_vue@3.4.38/node_modules/vite-ssg/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/vite-ssg@0.23.7_critters@0.0.24_vite@5.4.2_vue-router@4.4.3_vue@3.4.38/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-ssg@0.23.7_critters@0.0.24_vite@5.4.2_vue-router@4.4.3_vue@3.4.38/node_modules/vite-ssg/bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../../../../../vite-ssg@0.23.7_critters@0.0.24_vite@5.4.2_vue-router@4.4.3_vue@3.4.38/node_modules/vite-ssg/bin/vite-ssg.js" "$@"
fi

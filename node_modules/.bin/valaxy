#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/blog/valaxy-blog/node_modules/.pnpm/valaxy@0.19.9_postcss@8.4.41_typescript@5.5.3/node_modules/valaxy/bin/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/valaxy@0.19.9_postcss@8.4.41_typescript@5.5.3/node_modules/valaxy/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/valaxy@0.19.9_postcss@8.4.41_typescript@5.5.3/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/blog/valaxy-blog/node_modules/.pnpm/valaxy@0.19.9_postcss@8.4.41_typescript@5.5.3/node_modules/valaxy/bin/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/valaxy@0.19.9_postcss@8.4.41_typescript@5.5.3/node_modules/valaxy/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/valaxy@0.19.9_postcss@8.4.41_typescript@5.5.3/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../valaxy/bin/valaxy.mjs" "$@"
else
  exec node  "$basedir/../valaxy/bin/valaxy.mjs" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/blog/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_@vue+compiler-sfc@3.4.38_eslint@9.9.1_typescript@5.5.3/node_modules/@antfu/eslint-config/bin/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_@vue+compiler-sfc@3.4.38_eslint@9.9.1_typescript@5.5.3/node_modules/@antfu/eslint-config/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_@vue+compiler-sfc@3.4.38_eslint@9.9.1_typescript@5.5.3/node_modules/@antfu/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_@vue+compiler-sfc@3.4.38_eslint@9.9.1_typescript@5.5.3/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/blog/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_@vue+compiler-sfc@3.4.38_eslint@9.9.1_typescript@5.5.3/node_modules/@antfu/eslint-config/bin/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_@vue+compiler-sfc@3.4.38_eslint@9.9.1_typescript@5.5.3/node_modules/@antfu/eslint-config/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_@vue+compiler-sfc@3.4.38_eslint@9.9.1_typescript@5.5.3/node_modules/@antfu/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/@antfu+eslint-config@2.27.2_@vue+compiler-sfc@3.4.38_eslint@9.9.1_typescript@5.5.3/node_modules:/root/blog/valaxy-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
